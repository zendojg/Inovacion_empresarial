                        app:layout_constraintTop_toBottomOf="@+id/rbtCap6_41_1"
                        tools:ignore="TouchTargetSizeCheck" />

                    <RadioButton
                        android:id="@+id/rbtCap6_41_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="10dp"
                        android:padding="5dp"
                        android:fontFamily="sans-serif-condensed"
                        android:background="@drawable/background_shadow"
                        android:text="@string/tb41desc3"
                        android:layoutDirection="rtl"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/rbtCap6_41_2"
                        tools:ignore="TouchTargetSizeCheck" />

                    <RadioButton
                        android:id="@+id/rbtCap6_41_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="10dp"
                        android:padding="5dp"
                        android:fontFamily="sans-serif-condensed"
                        android:background="@drawable/background_shadow"
                        android:text="@string/tb41desc4"
