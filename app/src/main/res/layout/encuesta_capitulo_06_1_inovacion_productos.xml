                    app:layout_constraintTop_toBottomOf="@+id/lb40txt">

                    <TextView
                        android:layout_gravity="fill"
                        android:fontFamily="sans-serif-condensed"
                        android:paddingHorizontal="10dp"
                        android:layout_margin="2dp"
                        android:textStyle="bold"
                        android:gravity="center_vertical"
                        android:text="@string/p40tb1" />

                    <RadioGroup
                        android:id="@+id/rgroupCap6_40_1"
                        android:layout_gravity="fill"
                        android:layout_margin="2dp"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/rbtCap6_40_1_si"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:fontFamily="sans-serif-condensed"
                            android:text="@string/yes"
                            tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />

                        <RadioButton
                            android:id="@+id/rbtCap6_40_1_no"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:fontFamily="sans-serif-condensed"
                            android:text="@string/not"
                            tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />
                    </RadioGroup>

                    <TextView
                        android:layout_gravity="fill"
                        android:fontFamily="sans-serif-condensed"
                        android:paddingHorizontal="10dp"
                        android:layout_margin="2dp"
                        android:textStyle="bold"
                        android:gravity="center_vertical"
                        android:text="@string/p40tb2" />

                    <RadioGroup
                        android:id="@+id/rgroupCap6_40_2"
                        android:layout_gravity="fill"
                        android:layout_margin="2dp"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/rbtCap6_40_2_si"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:fontFamily="sans-serif-condensed"
                            android:text="@string/yes"
                            tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />

                        <RadioButton
                            android:id="@+id/rbtCap6_40_2_no"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:fontFamily="sans-serif-condensed"
                            android:text="@string/not"
                            tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />
                    </RadioGroup>
                </GridLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layoutCap6_41"
