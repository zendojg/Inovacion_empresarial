                        android:layout_height="wrap_content"
                        android:hint="@string/porcentaje"
                        android:visibility="invisible"
                        tools:visibility="visible"
                        app:layout_constraintHorizontal_weight="1"
                        app:layout_constraintEnd_toEndOf="parent"
