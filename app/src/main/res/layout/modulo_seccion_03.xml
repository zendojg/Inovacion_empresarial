                            android:layout_height="48dp"
                            android:backgroundTint="@color/white"
                            android:fontFamily="sans-serif-condensed"
                            android:gravity="center"
                            android:inputType="number"
                            android:maxLength="2"
                            android:textSize="14sp"
                            tools:ignore="TouchTargetSizeCheck" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <TextView
                        android:id="@+id/txtm3pc"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginStart="5dp"
                        android:padding="10dp"
                        android:background="@drawable/background_shadow_low"
                        android:fontFamily="sans-serif-condensed"
                        android:text="@string/p4m1c"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txtSecc03_4_bly"
                        tools:ignore="TextContrastCheck" />

                    <RadioGroup
                        android:id="@+id/rgroupSecc03_4_c"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintHorizontal_weight="2"
                        app:layout_constraintBottom_toBottomOf="@+id/txtSecc03_4_cly"
                        app:layout_constraintEnd_toStartOf="@+id/txtSecc03_4_cly"
                        app:layout_constraintStart_toStartOf="@+id/txtm3pc"
                        app:layout_constraintTop_toTopOf="@+id/txtSecc03_4_cly">

                        <RadioButton
                            android:id="@+id/rbtSecc03_4_c_si"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_weight="2"
                            android:fontFamily="sans-serif-condensed"
                            android:text="@string/yes"
                            tools:ignore="DuplicateSpeakableTextCheck" />

                        <RadioButton
                            android:id="@+id/rbtSecc03_4_c_no"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:fontFamily="sans-serif-condensed"
                            android:text="@string/not" />

                    </RadioGroup>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/txtSecc03_4_cly"
